CATEGORIES=computers-code;

    #!/bin/sed -f
    #DTPD#
    #txt2html Cleans up HTML control characters <.",&,>
    s/\&/\&amp;/g;
    s/\"/\&quot;/g;
    s/</\&lt;/g;
    s/>/\&gt;/g;
    s/\\/\&\#0092;/g;
